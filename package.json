  },
  "homepage": "https://github.com/cssmenumaker/jQuery-Plugin-Responsive-Drop-Down#readme",
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "nodemon": "^2.0.2"
  },
  "dependencies": {
    "express": "^4.17.3",
    "pug": "^2.0.4"
  }
}
